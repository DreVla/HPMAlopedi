<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hpmtutorial.hpmalopedi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/alopedi2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/alopedi2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.fragments.ContainerActivity">
        </activity>
        <activity android:name=".view.AddNewChildActivity" />
        <activity android:name=".view.HomeActivity"></activity>
        <activity android:name=".view.ConfirmActivity" />
        <activity android:name=".view.ValidationActivity" />
        <activity android:name=".view.SplashActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>